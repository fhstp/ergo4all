  "@tip_handAndArm_label": {
    "description": "Short label text for buttons referencing this tip"
  },
  "tip_handAndArm_summary": "Keep your wrists straight and avoid overextension. Move smoothly and without using excessive force. Support your forearms and keep your hands at elbow height. Change your grip regularly and do relaxation exercises.",
  "tip_lift_and_carry_label": "Lift and carry",
  "@tip_lift_and_carry_label": {
    "description": "Short label text for buttons referencing this tip"
  },
